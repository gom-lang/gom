import io;

type Point = struct {
    x: i8,
    y: i8
};

let GLOBAL = 1;

fn square(x: i8): i8 {
    return x * x;
}

fn add(a: i8, b: i8): i8 {
    return a + b;
}

/*
    Print numbers from 0 to n
*/
fn print_to_n(n: i8) {
    let i = 0;
    for(; i < n; i = i + 1) {
        io.log("i:", i);
    }
}

fn main() {
    let a = 1 + 2, b = 2;

    io.log("Sum:", add(1, (1 + b / 2)));
    io.log("Global:", GLOBAL);

    a = 2;
    io.log("Square:", square(a));

    print_to_n(5);
}